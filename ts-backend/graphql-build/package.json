    "ts-jest": "^26.4.4",
    "ts-node": "9.0.0",
    "typescript": "4.0.5"
  },
  "dependencies": {
    "@gql2ts/from-schema": "^1.10.1",
    "bcryptjs": "^2.4.3",
    "connect-redis": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "faker": "^5.1.0",
    "global": "^4.4.0",
    "gql2ts": "^1.10.1",
